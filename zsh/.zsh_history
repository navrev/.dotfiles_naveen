         /var/log/syslog\
         /var/log/remote-syslog\
         /var/log/auth.log\
         /var/log/kern.log\
         /var/log/mysql.log\
         /var/log/purity/snmpd.log\
         /var/log/mail.info\
         /var/log/mail.warn\
         /var/log/mail.err\
         /var/log/mail.log\
         /var/log/daemon.log\
         /var/log/user.log\
         /var/log/lpr.log\
         /var/log/cron.log\
         /var/log/debug\
         /var/log/messages\
         {\
               rotate 240\
               hourly\
               missingok\
               ifempty\
               delaycompress\
               compress\
               sharedscripts\
               postrotate\
                  service syslog-ng reload > /dev/null\
               endscript\
         }\
