  \      &filetype ==? 'denite'           ? ' ' :\
  \      &filetype ==? 'tagbar'           ? ' ' :\
  \      &filetype =~? '\v^mundo(diff)?$' ? ' ' :\
  \      s:lightline_is_lean() || s:lightline_is_plain() ? ' '  :\
  \      printf('%d:%d  %d%%', line('.'), col('.'), 100*line('.')/line('$'))\
endfunction\
